{
  "name": "rhs-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "format": "prettier --check './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "format:fix": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "cypress:run": "cypress run",
    "cypress:run:component": "cypress run --component",
    "cypress:run:e2e": "cypress run --e2e",
    "cypress:open": "cypress open",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.1.0",
    "@julr/vite-plugin-validate-env": "^0.2.2",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.1",
    "@tanstack/react-query": "^4.29.3",
    "axios": "^1.4.0",
    "i18next": "^23.2.3",
    "i18next-browser-languagedetector": "^7.1.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-i18next": "^13.0.1",
    "react-router-dom": "^6.10.0",
    "recoil": "^0.7.7",
    "vite-plugin-svgr": "^3.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@julr/vite-plugin-validate-env": "^0.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-infinite-scroller": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "cypress": "^12.10.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
